        <div class="container mx-auto px-4">
            <div class="grid grid-cols-1 lg:grid-cols-2 items-start">
                <div
                    class="hidden lg:grid grid-cols-2 gap-4 items-stretch max-w-[580px]"
                >
                    <Image
                        class="rounded-3xl w-full hover-up"
                        src={iSignup1}
                        alt="Sign in illustration"
                    />
                    <Image
                        class="rounded-3xl w-full hover-up"
                        src={iSignup2}
                        alt="Sign in illustration"
                    />
                    <Image
                        class="rounded-3xl w-full hover-up"
                        src={iSignup3}
                        alt="Sign in illustration"
                    />
                    <Image
                        class="rounded-3xl w-full hover-up"
                        src={iSignup4}
                        alt="Sign in illustration"
                    />
                    <Image
                        class="rounded-3xl w-full hover-up col-span-2"
                        src={iSignup5}
                        alt="Sign in illustration"
                    />
